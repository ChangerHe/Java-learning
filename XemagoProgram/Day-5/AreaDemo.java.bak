//求圆（circle）、矩形（rectangle）、三角形（triangle）的面积
class circle
{
	private Integer r;
	public void getInt(Integer r)
	{
		this.r = r;
	}
	public Double getArea( )
	{
		return 3.14 * r * r;
	}
}
class  AreaDemo
{
	public static void main(String[] args) 
	{
		System.out.println(new circle(4).getArea());
	}
}
